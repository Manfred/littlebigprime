<div class="column">
  <%= form_with model: @session, url: session_path do |form| %>
    <%= form.error :target %>
    <%= form.field :password do %>
      <%= form.label_with_error :password, t('.password') %>
      <%= form.password_field :password, autocomplete: 'current-password' %>
    <% end %>
    <div class="submit">
      <%= form.button t('.login') %>
    </div>
  <% end %>
</div>
